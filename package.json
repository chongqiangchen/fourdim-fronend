    "@types/react": "^18.0.14",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.5",
    "@types/react-lazy-load-image-component": "^1.5.2",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "craco-alias": "^2.1.3",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "19.0.4",
