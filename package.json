    "lodash": "^4.17.21",
    "notistack": "^2.0.5",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.34.0",
    "react-jazzicon": "^1.0.4",
    "react-lazy-load-image-component": "^1.5.4",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
