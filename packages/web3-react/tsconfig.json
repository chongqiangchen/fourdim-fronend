{
  "extends": "../tsconfig/react-lib.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "target": "es5",
    "paths": {}
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
